7:"virtual";s:4:"null";}s:12:"default_data";a:2:{s:4:"norm";s:4:"null";s:4:"view";s:0:"";}s:14:"submitted_data";a:1:{s:4:"norm";s:108:"Breastfeeding can halve the risk of post-natal depression, according to a large study of 14,000 new mothers.";}s:6:"errors";a:0:{}s:8:"children";a:0:{}}s:8:"category";a:11:{s:2:"id";s:28:"itr_newsbundle_news_category";s:4:"name";s:8:"category";s:4:"type";s:6:"entity";s:10:"type_class";s:44:"Symfony\Bridge\Doctrine\Form\Type\EntityType";s:12:"synchronized";s:4:"true";s:14:"passed_options";a:4:{s:4:"attr";s:23:"[class => form-control]";s:5:"class";s:19:"NewsBundle:Category";s:11:"empty_value";s:20:"Choose news category";s:8:"required";s:4:"true";}s:16:"resolved_options";a:50:{s:6:"action";s:0:"";s:4:"attr";s:23:"[class => form-control]";s:15:"auto_initialize";s:4:"true";s:10:"block_name";s:4:"null";s:12:"by_reference";s:4:"true";s:18:"cascade_validation";s:5:"false";s:11:"choice_list";s:64:"Object(Symfony\Bridge\Doctrine\Form\ChoiceList\EntityChoiceList)";s:7:"choices";s:4:"null";s:5:"class";s:19:"NewsBundle:Category";s:8:"compound";s:5:"false";s:11:"constraints";s:2:"[]";s:15:"csrf_field_name";s:6:"_token";s:12:"csrf_message";s:59:"The CSRF token is invalid. Please try to resubmit the form.";s:15:"csrf_protection";s:4:"true";s:13:"csrf_provider";s:82:"Object(Symfony\Component\Form\Extension\Csrf\CsrfProvider\CsrfTokenManagerAdapter)";s:13:"csrf_token_id";s:4:"null";s:18:"csrf_token_manager";s:78:"Object(Symfony\Component\Form\Extension\Csrf\CsrfProvider\CsrfProviderAdapter)";s:10:"data_class";s:4:"null";s:8:"disabled";s:5:"false";s:2:"em";s:34:"Object(Doctrine\ORM\EntityManager)";s:10:"empty_data";s:0:"";s:11:"empty_value";s:20:"Choose news category";s:14:"error_bubbling";s:5:"false";s:13:"error_mapping";s:2:"[]";s:8:"expanded";s:5:"false";s:20:"extra_fields_message";s:42:"This form should not contain extra fields.";s:8:"group_by";s:4:"null";s:12:"inherit_data";s:5:"false";s:9:"intention";s:4:"null";s:15:"invalid_message";s:24:"This value is not valid.";s:26:"invalid_message_parameters";s:2:"[]";s:5:"label";s:4:"null";s:10:"label_attr";s:2:"[]";s:6:"loader";s:4:"null";s:6:"mapped";s:4:"true";s:10:"max_length";s:4:"null";s:6:"method";s:4:"POST";s:8:"multiple";s:5:"false";s:7:"pattern";s:4:"null";s:21:"post_max_size_message";s:69:"The uploaded file was too large. Please try to upload a smaller file.";s:17:"preferred_choices";s:2:"[]";s:8:"property";s:4:"null";s:13:"property_path";s:4:"null";s:13:"query_builder";s:4:"null";s:9:"read_only";s:5:"false";s:8:"required";s:4:"true";s:18:"translation_domain";s:4:"null";s:4:"trim";s:4:"true";s:17:"validation_groups";s:4:"null";s:7:"virtual";s:4:"null";}s:12:"default_data";a:2:{s:4:"norm";s:4:"null";s:4:"view";s:0:"";}s:14:"submitted_data";a:2:{s:4:"norm";s:38:"Object(ITR\NewsBundle\Entity\Category)";s:4:"view";s:1:"4";}s:6:"errors";a:0:{}s:8:"children";a:0:{}}s:7:"content";a:11:{s:2:"id";s:27:"itr_newsbundle_news_content";s:4:"name";s:7:"content";s:4:"type";s:8:"textarea";s:10:"type_class";s:55:"Symfony\Component\Form\Extension\Core\Type\TextareaType";s:12:"synchronized";s:4:"true";s:14:"passed_options";a:1:{s:5:"label";s:1:" ";}s:16:"resolved_options";a:38:{s:6:"action";s:0:"";s:4:"attr";s:2:"[]";s:15:"auto_initialize";s:4:"true";s:10:"block_name";s:4:"null";s:12:"by_reference";s:4:"true";s:18:"cascade_validation";s:5:"false";s:8:"compound";s:5:"false";s:11:"constraints";s:2:"[]";s:15:"csrf_field_name";s:6:"_token";s:12:"csrf_message";s:59:"The CSRF token is invalid. Please try to resubmit the form.";s:15:"csrf_protection";s:4:"true";s:13:"csrf_provider";s:82:"Object(Symfony\Component\Form\Extension\Csrf\CsrfProvider\CsrfTokenManagerAdapter)";s:13:"csrf_token_id";s:4:"null";s:18:"csrf_token_manager";s:78:"Object(Symfony\Component\Form\Extension\Csrf\CsrfProvider\CsrfProviderAdapter)";s:10:"data_class";s:4:"null";s:8:"disabled";s:5:"false";s:10:"empty_data";s:15:"Object(Closure)";s:14:"error_bubbling";s:5:"false";s:13:"error_mapping";s:2:"[]";s:20:"extra_fields_message";s:42:"This form should not contain extra fields.";s:12:"inherit_data";s:5:"false";s:9:"intention";s:4:"null";s:15:"invalid_message";s:24:"This value is not valid.";s:26:"invalid_message_parameters";s:2:"[]";s:5:"label";s:1:" ";s:10:"label_attr";s:2:"[]";s:6:"mapped";s:4:"true";s:10:"max_length";s:4:"null";s:6:"method";s:4:"POST";s:7:"pattern";s:4:"null";s:21:"post_max_size_message";s:69:"The uploaded file was too large. Please try to upload a smaller file.";s:13:"property_path";s:4:"null";s:9:"read_only";s:5:"false";s:8:"required";s:4:"true";s:18:"translation_domain";s:4:"null";s:4:"trim";s:4:"true";s:17:"validation_groups";s:4:"null";s:7:"virtual";s:4:"null";}s:12:"default_data";a:2:{s:4:"norm";s:4:"null";s:4:"view";s:0:"";}s:14:"submitted_data";a:1:{s:4:"norm";s:10894:"<blockquote>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">However, there is a large increase in the risk of depression in women planning to breastfeed who are then unable to do so.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">The study, published in the journal Maternal and Child Health, called for more support for women unable to breastfeed.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">A parenting charity said mental health was a "huge issue" for many mothers.</span></p>
<p class="MsoNormal" style="margin: 0cm 0cm 0.0001pt 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">The health benefits of breastfeeding to the baby are clear-cut and the World Health Organization recommends feeding a child nothing but breast milk for the first six months.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">However, researchers at the University of Cambridge said the impact on the mother was not as clearly understood.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><strong><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#505050;mso-ansi-language:EN-US;mso-fareast-language:RU">'Highest risk'</span></strong></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">One in 10 women will develop depression after the birth of their child.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">The researchers analysed data from 13,998 births in the south-west of England.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">It showed that, out of women who were planning to breastfeed, there was a 50% reduction in the risk of post-natal depression if they started breastfeeding.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">But the risk of depression more than doubled among women who wanted to, but were not able to, breastfeed.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Maria Iacovou, one of the researchers, told the BBC: "Breastfeeding does appear to have a protective effect, but there's the other side of the coin as well.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">"Those who wanted to and didn't end up breastfeeding had the highest risk of all the groups."</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">The benefit increased for each week of breastfeeding up to one month.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">However, any longer term impact on post-natal depression could not be determined due to the small number of women in the study who were still breast feeding several months after the birth.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><strong><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#505050;mso-ansi-language:EN-US;mso-fareast-language:RU">Action needed</span></strong></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Iacovou said the health profession needed to pay attention.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">She added: "It is right to tell mothers it's right to breastfeed, there's so many benefits, but the thing we need to rethink is giving more support to those who did want to breastfeed and to recognise those who are unable to, are at substantially elevated risk and to make sure health visitors keep an eye on these women."</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">Possible explanations for the beneficial effect include the release of feel-good hormones when milk is produced.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Iacovou added that social or psychological factors such as feelings of "failing as a mother" were also contributing.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">She says the improving the health of the mother will also help the baby.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Rosemary Dodds, a senior policy adviser at parenting charity NCT, said: "Mothers often experience pressures after the birth such as pain, shortage of sleep and anxiety.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">"Breastfeeding can help to relax mothers and reduce stress, so it might play a part in preventing mental health issues developing.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">"We welcome further research into this subject as perinatal mental health is a huge issue for many mothers. </span><span style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:
&quot;Times New Roman&quot;;color:#333333;mso-fareast-language:RU">At least one in ten suffer with postnatal depression."</span></p>
</blockquote>";}s:6:"errors";a:0:{}s:8:"children";a:0:{}}s:6:"submit";a:4:{s:14:"submitted_data";a:1:{s:4:"norm";s:4:"null";}s:6:"errors";a:0:{}s:12:"synchronized";s:4:"true";s:8:"children";a:0:{}}}}}s:13:"forms_by_hash";a:7:{s:32:"00000000760265e600000000d443585f";R:1112;s:32:"00000000760265e500000000d443585f";R:1168;s:32:"000000007602666200000000d443585f";R:1222;s:32:"000000007602666500000000d443585f";R:1276;s:32:"000000007602666800000000d443585f";R:1330;s:32:"000000007602666b00000000d443585f";R:1400;s:32:"000000007602667b00000000d443585f";R:1454;}s:9:"nb_errors";i:0;}}s:8:"security";C:65:"Symfony\Bundle\SecurityBundle\DataCollector\SecurityDataCollector":213:{a:5:{s:7:"enabled";b:1;s:13:"authenticated";b:0;s:11:"token_class";s:74:"Symfony\Component\Security\Core\Authentication\Token\UsernamePasswordToken";s:4:"user";s:5:"admin";s:5:"roles";a:1:{i:0;s:10:"ROLE_ADMIN";}}}s:11:"swiftmailer";C:67:"Symfony\Bundle\SwiftmailerBundle\DataCollector\MessageDataCollector":77:{a:3:{s:6:"mailer";a:0:{}s:12:"messageCount";i:0;s:13:"defaultMailer";s:0:"";}}s:2:"db";C:66:"Doctrine\Bundle\DoctrineBundle\DataCollector\DoctrineDataCollector":13056:{a:5:{s:7:"queries";a:1:{s:7:"default";a:6:{i:1;a:5:{s:3:"sql";s:246:"SELECT u0_.id AS id0, u0_.user_name AS user_name1, u0_.user_email AS user_email2, u0_.user_password AS user_password3, u0_.user_role AS user_role4, u0_.user_active AS user_active5, u0_.user_hash AS user_hash6 FROM User u0_ WHERE u0_.user_name = ?";s:6:"params";a:1:{i:0;s:5:"admin";}s:5:"types";a:1:{i:0;i:2;}s:11:"executionMS";d:0.0030000209808349609;s:11:"explainable";b:1;}i:2;a:5:{s:3:"sql";s:136:"SELECT t0.id AS id1, t0.access_hash AS access_hash2, t0.expires AS expires3, t0.user AS user4 FROM PasswordRecovery t0 WHERE t0.user = ?";s:6:"params";a:1:{i:0;i:13;}s:5:"types";a:1:{i:0;N;}s:11:"executionMS";d:0;s:11:"explainable";b:1;}i:3;a:5:{s:3:"sql";s:72:"SELECT t0.id AS id1, t0.category_name AS category_name2 FROM Category t0";s:6:"params";a:0:{}s:5:"types";a:0:{}s:11:"executionMS";d:0;s:11:"explainable";b:1;}i:4;a:5:{s:3:"sql";s:19:""START TRANSACTION"";s:6:"params";a:0:{}s:5:"types";N;s:11:"executionMS";d:0.00099992752075195312;s:11:"explainable";b:1;}i:5;a:5:{s:3:"sql";s:111:"INSERT INTO News (title, author, publication_date, description, content, category_id) VALUES (?, ?, ?, ?, ?, ?)";s:6:"params";a:6:{i:1;s:56:"Breastfeeding 'cuts depression risk', according to study";i:2;s:15:"James Gallagher";i:3;s:19:"2014-08-20 12:37:51";i:4;s:108:"Breastfeeding can halve the risk of post-natal depression, according to a large study of 14,000 new mothers.";i:5;s:10894:"<blockquote>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">However, there is a large increase in the risk of depression in women planning to breastfeed who are then unable to do so.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">The study, published in the journal Maternal and Child Health, called for more support for women unable to breastfeed.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">A parenting charity said mental health was a "huge issue" for many mothers.</span></p>
<p class="MsoNormal" style="margin: 0cm 0cm 0.0001pt 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">The health benefits of breastfeeding to the baby are clear-cut and the World Health Organization recommends feeding a child nothing but breast milk for the first six months.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt; text-indent: 35.4pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">However, researchers at the University of Cambridge said the impact on the mother was not as clearly understood.</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><strong><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#505050;mso-ansi-language:EN-US;mso-fareast-language:RU">'Highest risk'</span></strong></p>
<p class="MsoNormal" style="margin-bottom: 0.0001pt;"><span lang="EN-US" style="font-size: 14pt; font-family: 'Times New Roman', serif;">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">One in 10 women will develop depression after the birth of their child.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">The researchers analysed data from 13,998 births in the south-west of England.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">It showed that, out of women who were planning to breastfeed, there was a 50% reduction in the risk of post-natal depression if they started breastfeeding.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">But the risk of depression more than doubled among women who wanted to, but were not able to, breastfeed.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Maria Iacovou, one of the researchers, told the BBC: "Breastfeeding does appear to have a protective effect, but there's the other side of the coin as well.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">"Those who wanted to and didn't end up breastfeeding had the highest risk of all the groups."</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">The benefit increased for each week of breastfeeding up to one month.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">However, any longer term impact on post-natal depression could not be determined due to the small number of women in the study who were still breast feeding several months after the birth.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><strong><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#505050;mso-ansi-language:EN-US;mso-fareast-language:RU">Action needed</span></strong></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:16.0pt;mso-bidi-font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">&nbsp;</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Iacovou said the health profession needed to pay attention.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">She added: "It is right to tell mothers it's right to breastfeed, there's so many benefits, but the thing we need to rethink is giving more support to those who did want to breastfeed and to recognise those who are unable to, are at substantially elevated risk and to make sure health visitors keep an eye on these women."</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">Possible explanations for the beneficial effect include the release of feel-good hormones when milk is produced.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Dr Iacovou added that social or psychological factors such as feelings of "failing as a mother" were also contributing.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">She says the improving the health of the mother will also help the baby.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">Rosemary Dodds, a senior policy adviser at parenting charity NCT, said: "Mothers often experience pressures after the birth such as pain, shortage of sleep and anxiety.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;text-indent:
35.4pt;mso-line-height-alt:13.5pt"><span lang="EN-US" style="font-size:14.0pt;
font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:&quot;Times New Roman&quot;;
color:#333333;mso-ansi-language:EN-US;mso-fareast-language:RU">"Breastfeeding can help to relax mothers and reduce stress, so it might play a part in preventing mental health issues developing.</span></p>
<p class="MsoNormal" style="margin-bottom:0cm;margin-bottom:.0001pt;mso-line-height-alt:
13.5pt"><span lang="EN-US" style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;
mso-fareast-font-family:&quot;Times New Roman&quot;;color:#333333;mso-ansi-language:EN-US;
mso-fareast-language:RU">"We welcome further research into this subject as perinatal mental health is a huge issue for many mothers. </span><span style="font-size:14.0pt;font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;mso-fareast-font-family:
&quot;Times New Roman&quot;;color:#333333;mso-fareast-language:RU">At least one in ten suffer with postnatal depression."</span></p>
