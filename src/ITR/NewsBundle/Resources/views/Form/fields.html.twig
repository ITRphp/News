{% extends "NewsBundle::layout.html.twig" %}

{% block form_errors %}
{% spaceless %}
    {% if errors|length > 0 %}
        
            {% for error in errors %}
                <div class="error" style="display: inline"> {{ error.messageTemplate|trans(error.messageParameters, 'validators') }}</div>
            {% endfor %}
       
    {% endif %}
{% endspaceless %}
{% endblock form_errors %}